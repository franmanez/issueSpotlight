msgstr "Author"

msgid "plugins.generic.issueSpotlight.geo.affiliation"
msgstr "Affiliation"

msgid "plugins.generic.issueSpotlight.geo.article"
msgstr "Article"

msgid "plugins.generic.issueSpotlight.geo.noAffiliation"
msgstr "No affiliation"

